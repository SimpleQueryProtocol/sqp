<suite name="native-transbase">
    <parameter name="backendConfiguration" value="native-transbase" />
    <test name="Native Transbase Backend Integration" parallel="false">
        <groups>
            <run>
                <exclude name="jdbc-postgres-only" />
                <exclude name="native-postgres-only" />
                <exclude name="without-native-transbase" />
            </run>
        </groups>
        <packages>
            <package name="io.sqp.client.*" />
        </packages>
    </test>
</suite>
